{
    "name": "Breast Cancer Detect",
    "n_gpu": 1,

    "arch": {
        "type": "BreastCancerDetectionModel",
        "args": 
        {
            "model_name" : "seresnext50_32x4d", 
            "cols_num_classes": [2, 2, 6, 1, 2, 2, 4, 5, 2, 10, 10]
        }   
    },
    "data_loader": {
        "type": "BreastCancerDataloader",
        "args":{
            "data_dir": "/kaggle/input/breast-cancer-fitted-png-384x384",
            "df_file_path": "/kaggle/input/rsna-breast-cancer-detection/train.csv",
            "batch_size": 16,
            "shuffle": true,
            "validation_split": 0.1,
            "num_workers": 2
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.001,
            "weight_decay": 0,
            "amsgrad": true
        }
    },
    "loss": "cancer_loss",
    "metrics": [
        "pfbeta", "optimal_f1"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 2,

        "save_dir": "saved/",
        "save_period": 1,
        "verbosity": 2,
        
        "monitor": "min val_loss",
        "early_stop": 10

    },
    "PAT": "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"
}
